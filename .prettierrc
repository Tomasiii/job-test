{
	"arrowParens": "avoid",
	"printWidth": 120,
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@pages/(.*)$",
		"^@layout/(.*)$",
		"^@context/(.*)$",
		"^@components",
		"^@store",
		"^@components/(.*)$",
		"^@ui-kit/(.*)$",
		"^@redux/(.*)$",
		"^@hooks/(.*)$",
		"^@services/(.*)$",
		"^@assets/(.*)$",
		"^@styles/(.*)$",
		"^@utils/(.*)$",
		"^@config/(.*)$",
		"^@store/(.*)$",
		"^@type/(.*)$",
		"^@data/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true
}
